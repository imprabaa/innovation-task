public class SumEvenNumbers {
    public static void main(String[] args) {
        int sum = 0;

       
        for (int i = 1; i <= 11; i++) {
            if (i % 2 == 0) { // Check if the number is even
                sum += i;
            }
        }

        
        System.out.println("The sum of all even numbers up to 11 is: " + sum);
    }
}
