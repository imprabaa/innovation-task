class Vehicle {

    public void start() {
        System.out.println("Vehicle is starting...");
    }
}


class Car extends Vehicle {
    @Override
    public void start() {
        System.out.println("Car sounds **ROARR**!");
    }
}


class Bike extends Vehicle {
    @Override
    public void start() {
        System.out.println("Bike sounds **VROOMM**!");
    }
}


public class Main {
    public static void main(String[] args) {
       
        Vehicle V1 = new Car();  
        Vehicle V2 = new Bike(); 
       
        V1.start();
        V2.start();
        
    }
}
